<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:org.gwtbootstrap3.client.ui"
	xmlns:h="urn:import:org.gwtbootstrap3.client.ui.html">
	<ui:style>
	.important {
		font-weight: bold;
	}
	</ui:style>
	<g:HTMLPanel>
		<b:Container fluid="true">
			<b:Row>
				<b:Column size="SM_12">
					<b:Panel>

						<b:PanelHeader>
							<b:Heading size="H4" text="User Questions" />
						</b:PanelHeader>

						<b:PanelBody>

							<b:PanelGroup b:id="accordion">

								<!-- TO RECONSTRUCT -->
								<b:Panel>
									<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion"
										dataTarget="#collapseOne" text="One">
									</b:PanelHeader>
									<b:PanelCollapse b:id="collapseOne">
										<b:PanelBody></b:PanelBody>
									</b:PanelCollapse>
								</b:Panel>
								<!-- END TO RECONSTRUCT -->

								<b:Panel>
									<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion"
										dataTarget="#collapseTwo" text="Two">
									</b:PanelHeader>
									<b:PanelCollapse b:id="collapseTwo">
										<b:PanelBody></b:PanelBody>
									</b:PanelCollapse>
								</b:Panel>

								<b:Panel>
									<b:PanelHeader dataToggle="COLLAPSE" dataParent="#accordion"
										dataTarget="#collapseThree" text="Three">
									</b:PanelHeader>
									<b:PanelCollapse b:id="collapseThree">
										<b:PanelBody></b:PanelBody>
									</b:PanelCollapse>
								</b:Panel>

							</b:PanelGroup>

						</b:PanelBody>

						<b:PanelFooter></b:PanelFooter>

					</b:Panel>
				</b:Column>
			</b:Row>
		</b:Container>
	</g:HTMLPanel>
</ui:UiBinder> 